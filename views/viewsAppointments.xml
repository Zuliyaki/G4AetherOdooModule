<odoo>
    <data>
        <!-- explicit list view definition -->
        
        <record model="ir.ui.view" id="aether.appointment.list">
            <field name="name">aether appointment list</field>
            <field name="model">aether.appointment</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Patient"/>
                    <field name="Date"/>
                    //<field name="value2"/>
                </tree>
            </field>
        </record>
 
        <!-- actions opening views on models -->
   
        <record model="ir.actions.act_window" id="aether.action_window_1">
            <field name="name">aether window</field>
            <field name="res_model">aether.aether</field>
            <field name="view_mode">tree,form</field>
        </record>
      
        <!-- actions -->
    
        <menuitem name="List" 
                  id="aether.menu_1_list" 
                  parent="aether.menu_1"
                  action="aether.action_window_4"/>
        
    </data>
</odoo>